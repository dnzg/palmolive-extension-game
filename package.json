{
  "name": "palmolive-shooter",
  "version": "0.0.1",
  "description": "Extension Game for Palmolive",
  "main": "index.js",
  "buildNumber": "21",
  "scripts": {
    "dev": "run-p start watch:css functions browser",
    "functions": "cd functions && npm run serve",
    "scss": "node-sass --output-style compressed -o public/css public/scss",
    "watch:css": "onchange 'public/scss' -- npm run scss",
    "start": "node index.js",
    "browser": "gulp browser-sync",
    "cert": "node bin/generate_cert.js server",
    "build:ext": "rm -rf ./out && cp -R ./public ./out && rm -R ./out/scss && npm run build:zip && rm -rf ./out",
    "build:zip": "cross-env node bin/zip-build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winstrike/hyperx-landing.git"
  },
  "author": "Winstrike Special Projects",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/winstrike/hyperx-landing/issues"
  },
  "homepage": "https://github.com/winstrike/hyperx-landing#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^8.2.4",
    "firebase-admin": "^9.4.2",
    "moment": "^2.29.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "archiver": "^5.2.0",
    "browser-sync": "^2.26.14",
    "cross-env": "^7.0.3",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0"
  }
}